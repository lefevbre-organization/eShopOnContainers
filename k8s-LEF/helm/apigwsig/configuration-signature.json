{
  "ReRoutes": [
    {
      "DownstreamScheme": "http",
      "UpstreamHttpMethod": [ "POST", "PUT", "GET", "DELETE", "PATCH" ],
      "DownstreamHostAndPorts": [
        {
          "Host": "signature-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/{version}/signservice/{everything}",
      "DownstreamPathTemplate": "/api/{version}/{everything}"
    },
    {
      "DangerousAcceptAnyServerCertificateValidator": true,
      "DownstreamScheme": "http",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamHostAndPorts": [
        {
          "Host": "userutils-api",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/api/{version}/utils/{everything}",
      "DownstreamPathTemplate": "/api/{version}/UserUtils/{everything}"
    }
  ],
  "GlobalConfiguration": {
    "RequestIdKey": "OcRequestId",
    "AdministrationPath": "/administration"
  }
}