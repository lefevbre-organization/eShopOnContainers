{
  "ConnectionString": "mongodb://localhost",
  "Database": "LexonApiDB",
  "Collection": "Users",
  "CollectionMasters": "Masters",
  "CollectionEvents": "IntegrationEventLog",
  "UseCustomizationData": false,
  "Serilog": {
    "SeqServerUrl": null,
    "LogstashgUrl": null,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.eShopOnContainers": "Information",
        "Lexon.API": "Information",
        "System": "Warning"
      }
    }
  },
  "AzureServiceBusEnabled": false,
  "AzureStorageEnabled": false,
  "SubscriptionClientName": "Lexon",
  "SubscriptionConsumerName": "LexonAPI",
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 52000000
    }
  },
  "EventBusRetryCount": 5,
  "EventBusConnection": "",
  "EventBusUserName": "",
  "EventBusPassword": "",
  "UseVault": false,
  "Vault": {
    "Name": "eshop",
    "ClientId": "your-clien-id",
    "ClientSecret": "your-client-secret"
  },
  "AllowedHosts": "*",
  "IdAppNavision": 1
}
